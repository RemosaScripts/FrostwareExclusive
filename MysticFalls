
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Mystic HUB | sinscity studios", "Synapse")



local Main = Window:NewTab("Main")
local Section = Main:NewSection("Main")

Section:NewButton("Delete Channel", "", function()
    	_G.DeleteChannel = toggle
	while _G.DeleteChannel == true do
		local Player = game.Players.LocalPlayer
		local Character = Player.Character

		local Channel = Character:WaitForChild("Channeling")
		Channel:Destroy()
		wait()
	end
end)

Section:NewButton("Loop Delete Channel", "", function()
    local Player = game.Players.LocalPlayer
		local Character = Player.Character

		local Channel = Character:WaitForChild("Channeling")
		Channel:Destroy()
		wait()
    
end)

Section:NewTextBox("Loop Channel", "Works Better On Other Exploiters", function(plr)
	print(plr)
		_G.CHANNEL = true 
	    while _G.CHANNEL == true do
	    local args = {
		[1] = "Channel",
		[2] = {
		    [1] = true,
		    [2] = game.Players[plr].Character
		}
	    }
	    game:GetService("ReplicatedStorage").Events.Witch:FireServer(unpack(args))

	    local args = {
		[1] = "Magic",
		[2] = {
		    [1] = "requestChanneling",
		    [2] = game.Players[plr].Character
		}
	    }
	    game:GetService("ReplicatedStorage").Events.Witch:FireServer(unpack(args))
	    wait()
	end
	
end)
Section:NewButton("Unragdoll", "", function()
    -- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide

local ohNumber1 = 1

game:GetService("ReplicatedStorage").Events.FallDamage:FireServer(ohNumber1)
end)

Section:NewButton("Push Everyone", "", function()
    print("Clicked")


local Plr = game.Players.LocalPlayer
for i, Player in pairs (game.Players:GetPlayers()) do

    if Player.Character ~= nil and Player ~= Plr then
        local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
        if mag < 1000 then
            local Target = Player.Character or Player.CharacterAdded:Wait()

local args = {
[1] = "Push",
[2] = {
    [1] = Target
}
}

game:GetService("ReplicatedStorage").Events.Witch:FireServer(unpack(args))

end
end
end

end)

Section:NewButton("Put Everyone to sleep", "", function()
        print("Clicked")


        local Plr = game.Players.LocalPlayer
        for i, Player in pairs (game.Players:GetPlayers()) do

            if Player.Character ~= nil and Player ~= Plr then
                local mag = (Player.Character.UpperTorso.Position - Plr.Character.UpperTorso.Position).magnitude
                if mag < 1000 then
                    local Target = Player.Character or Player.CharacterAdded:Wait()

                    local A1 = "Magic"
                    local A2 = 
                        {
                            [1] = "SleepSpell", 
                            [2] = Target
                        }
                    local Event = game:GetService("ReplicatedStorage").Events.Witch
                    Event:FireServer(A1, A2)

                end
            end
        end

         end)








